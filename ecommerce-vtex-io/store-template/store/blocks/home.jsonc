  "tab-content.item#home1": {
    "children": [
      "search-result-layout.customQuery#1"
    ],
    "props": {
      "tabId": "kits"
    }
  },
  
  "search-result-layout.customQuery#1": {
    "props": {
      "querySchema": {
        "skusFilter": "ALL_AVAILABLE",
        "simulationBehavior": "skip",
        "queryField": "kit upmedal",
        "mapField": "category-1",
        "maxItemsPerPage": 8
      }
    },
    "blocks": [
      "search-result-layout.desktop#home"
    ]
  },
  
