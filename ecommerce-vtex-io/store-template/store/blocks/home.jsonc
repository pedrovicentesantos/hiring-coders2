  "search-result-layout.desktop#home":{
    "children": [      
      "flex-layout.col#home-search",
      "search-fetch-more#home"
    ],
    "props": {
      "defaultGalleryLayout": "many",
      "blockClass": "home"
    }
  },

  "search-fetch-more#home": {
    "props": {
      "blockClass": "home"
    }
  },

  "flex-layout.col#home-search": { 
    "children": [
      "search-content#home"
    ],
    "props": {
      "blockClass": "home-search"
    }
  },

  "search-content#home": { 
    "blocks": ["gallery#home", "not-found"],
    "props": {
      "blockClass": "home"
    }
  },

  "gallery#home": {
    "props": {
      "layouts": [
        {
          "name": "many",
          "component": "ManyByLineSummary",
          "itemsPerRow": {
            "desktop": 2,
            "mobile": 1
          }
        }
      ],
      "ManyByLineSummary": "product-summary.shelf#home",
      "blockClass": "home"
    }
  },

  "product-summary.shelf#home": {
    "children": [
      "product-summary-name#home",
      "product-summary-image#home",
      "flex-layout.row#home-price"   
    ],
    "props": {
      "blockClass": "home"
    }
  },

  "flex-layout.row#home-price": {
    "children":[
      "product-selling-price#home",
      "link.product#product-page"
    ],
    "props": {
      "blockClass": "home-price",
      "marginTop": 5
    }
  },

  "product-selling-price#home": {
    "props": {
      "blockClass": "home"
    }
  },

  "product-summary-image#home": {
    "props": {
      "blockClass": "home"
    }
  },

  "product-summary-name#home": {
    "props": {
      "blockClass": "home"
    }
  },

  "link.product#product-page": {
    "props": {
      "href": "/{slug}/p",
      "label": "Ver detalhes",
      "displayMode": "button",
      "blockClass": "home"
    }
  },

  "slider-layout#home": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "showNavigationArrows": "desktopOnly",
      "blockClass": "carousel-home"
    },
    "children": ["link.product#slider1", "link.product#slider2"]
  },

  "link.product#slider1": {
    "props": {
      "href": "/woodtrophie/p",

      "blockClass": "slider-home-link"
    },
    "children": [
      "image#slider-home1"
    ]
  },

  "image#slider-home1": {
    "props": {
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/6iibxhv7jzzdn2749uhoxub2fdjf",
      "blockClass": "home-slider"
    }
  },

  "link.product#slider2": {
    "props": {
      "href": "/arqueiro/p",
      "blockClass": "slider-home-link"
    },
    "children": [
      "image#slider-home2"
    ]
  },

  "image#slider-home2": {
    "props": {
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/du3k6akoks0u3xe322aypnaaal8e",
      "blockClass": "home-slider"
    }
  }
