{
  "store.home": { 
    "blocks": [
      "responsive-layout.desktop",
      "responsive-layout.phone"
    ]
  },

  "responsive-layout.desktop": {
    "children": ["flex-layout.row#home"]
  },

  "flex-layout.row#home": { 
    "props": {
      "fullWidth": true,
      "blockClass": "home"
    },
    "children":["tab-layout#home"]
  },

  "tab-layout#home": {
    "children": [
      "tab-list#home", "slider-layout#home","tab-content#home"
    ],
    "props":{
      "defaultActiveTabId": "kits",
      "blockClass": "home"
    }
  },

  "tab-list#home": {
    "children": [
      "tab-list.item#home1",
      "tab-list.item#home2",
      "tab-list.item#home3",
      "tab-list.item#home4",
      "tab-list.item#home5",
      "tab-list.item#home6",
      "tab-list.item#home7",
      "tab-list.item#home8"
    ],
    "props": {
      "blockClass": "home"
    }
  },

  "tab-list.item#home1": {
    "props": {
      "tabId": "kits",
      "label": "Kits",
      "defaultActiveTab": true
    }
  },

  "tab-list.item#home2": {
    "props": {
      "tabId": "palmeiras",
      "label": "Palmeiras"
    }
  },

  "tab-list.item#home3": {
    "props": {
