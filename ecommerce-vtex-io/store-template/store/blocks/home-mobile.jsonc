{
  "responsive-layout.phone": {
    "children": ["flex-layout.row#home-mobile"]
  },

  "flex-layout.row#home-mobile": { 
    "props": {
      "fullWidth": true,
      "blockClass": "home-mobile"
    },
    "children":["tab-layout#home-mobile"]
  },

  "tab-layout#home-mobile": {
    "children": [
      "slider-layout#itens", "slider-layout#home-mobile","tab-content#home-mobile"
    ],
    "props":{
      "defaultActiveTabId": "kits",
      "blockClass": "home-mobile"
    }
  },

  "slider-layout#itens": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 2
      },
      "infinite": false,
      "showNavigationArrows": "never",
      "showPaginationDots": "never",
      "blockClass": "home-mobile-tab-list"
    },
    "children": [
      "tab-list.item#home-mobile1",
      "tab-list.item#home-mobile2",
      "tab-list.item#home-mobile3",
      "tab-list.item#home-mobile4",
      "tab-list.item#home-mobile5",
      "tab-list.item#home-mobile6",
      "tab-list.item#home-mobile7",
      "tab-list.item#home-mobile8"
    ]
  },

  "tab-list.item#home-mobile1": {
    "props": {
      "tabId": "kits",
      "label": "Kits",
      "defaultActiveTab": true,
      "blockClass": "tab-list-item-mobile"
    }
  },

  "tab-list.item#home-mobile2": {
    "props": {
