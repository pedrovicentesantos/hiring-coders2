  "search-result-layout.desktop#home-mobile":{
    "children": [      
      "flex-layout.col#home-mobile-search",
      "search-fetch-more#home-mobile"
    ],
    "props": {
      "defaultGalleryLayout": "many",
      "blockClass": "home-mobile"
    }
  },

  "search-fetch-more#home-mobile": {
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "flex-layout.col#home-mobile-search": { 
    "children": [
      "search-content#home-mobile"
    ],
    "props": {
      "blockClass": "home-mobile-search"
    }
  },

  "search-content#home-mobile": { 
    "blocks": ["gallery#home-mobile", "not-found"],
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "gallery#home-mobile": {
    "props": {
      "layouts": [
        {
          "name": "many-mobile",
          "component": "ManyByLineSummary",
          "itemsPerRow": {
            "desktop": 2,
            "mobile": 1
          }
        }
      ],
      "ManyByLineSummary": "product-summary.shelf#home-mobile",
      "blockClass": "home-mobile"
    }
  },

  "product-summary.shelf#home-mobile": {
    "children": [
      "product-summary-name#home-mobile",
      "product-summary-image#home-mobile",
      "flex-layout.row#home-mobile-price"   
    ],
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "flex-layout.row#home-mobile-price": {
    "children":[
      "product-selling-price#home-mobile",
      "link.product#product-page-mobile"
    ],
    "props": {
      "blockClass": "home-mobile-price",
      "marginTop": 5
    }
  },

  "product-selling-price#home-mobile": {
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "product-summary-image#home-mobile": {
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "product-summary-name#home-mobile": {
    "props": {
      "blockClass": "home-mobile"
    }
  },

  "link.product#product-page-mobile": {
    "props": {
      "href": "/{slug}/p",
      "label": "Ver detalhes",
      "displayMode": "button",
      "blockClass": "home-mobile"
    }
  },

  "slider-layout#home-mobile": {
    "props": {
      "itemsPerPage": {
        "desktop": 1,
        "tablet": 1,
        "phone": 1
      },
      "infinite": true,
      "blockClass": "carousel-home-mobile"
    },
    "children": ["link.product#slider-mobile1", "link.product#slider-mobile2"]
  },

  "link.product#slider-mobile1": {
    "props": {
      "href": "/palmeiraspalestraitalia/p",

      "blockClass": "slider-home-mobile-link"
    },
    "children": [
      "image#slider-home-mobile1"
    ]
  },

  "image#slider-home-mobile1": {
    "props": {
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/6iibxhv7jzzdn2749uhoxub2fdjf",
      "blockClass": "home-mobile-slider"
    }
  },

  "link.product#slider-mobile2": {
    "props": {
      "href": "/arqueiro/p",
      "blockClass": "slider-home-mobile-link"
    },
    "children": [
      "image#slider-home-mobile2"
    ]
  },

  "image#slider-home-mobile2": {
    "props": {
      "src": "https://maratonavirtual.s3.sa-east-1.amazonaws.com/du3k6akoks0u3xe322aypnaaal8e",
